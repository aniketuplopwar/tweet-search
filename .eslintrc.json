{
  "plugins": ["babel","react","promise"],
  "parser" : "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "key-spacing": "off",
    "jsx-quotes": [2, "prefer-single"],
    "max-len": [2, 120, 2],
    "object-curly-spacing": [2, "always"],
    "comma-dangle": "off"
  }
}